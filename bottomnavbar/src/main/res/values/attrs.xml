<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomNavBar">
        <attr name="bn_background_color" format="integer"/>
        <attr name="bn_item_color" format="color"/>
        <attr name="bn_menu" format="integer"/>

        <attr name="bn_curve_rounded_corner_radius" format="dimension"/>
        <attr name="bn_curve_margin" format="dimension"/>
        <attr name="bn_curve_vertical_offset" format="dimension"/>

        <attr name="bn_fab_size" format="enum">
            <enum name="normal" value="0"/>
            <enum name="mini" value="1"/>
            <enum name="auto" value="-1"/>
        </attr>
        <attr name="bn_fab_menu_index" format="integer"/>
        <attr name="bn_fab_background_color" format="integer"/>
        <attr name="bn_fab_icon_color" format="color"/>

    </declare-styleable>
</resources>